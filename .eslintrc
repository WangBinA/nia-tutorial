{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "globals": {
    "$": true
  },
  "rules": {
    "no-unused-vars": ["error", {"vars": "all", "args": "none"}],
    "no-console": "off",
    "comma-dangle": ["off", "never"],
    "no-self-assign": "error",

    // "valid-jsdoc": ["warn", {
    //   "requireReturn": false,
    //   "requireReturnType": false,
    //   "requireParamDescription": false,
    //   "requireReturnDescription": false
    // }]
  }
}
